# Database
DATABASE_URL="mysql://ecommerce_user:ecommerce_password@localhost:3306/ecommerce_db"

# Redis
REDIS_URL="redis://localhost:6379"

# JWT
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="7d"

# Server
PORT=3000
NODE_ENV="development"
HOST="0.0.0.0"

# CORS
CORS_ORIGIN="http://localhost:3000,http://localhost:3001"

# Logging
LOG_LEVEL="info"
LOG_FILE="logs/app.log"

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW="15m"

# Security
BCRYPT_ROUNDS=12